/**index.wxss**/
.container{
    padding:0;
    background: #eee
}
.banners {
  width: 100%;
  height: 200px;
  background: #fff;
}

.banner{
  position: relative;
}

.banner-image{
  width: 100%;
  height: 100%;
}

.banner-title{
  color: #FFF;
  position: absolute;
  width: 100%;
  padding-left: 16rpx;
  padding-right: 16rpx; 
  bottom: 30rpx;
  font-size: 16rpx;
   left: 0;
}
.view-list{
    width:100%;
   
}
.view-title{
    position: relative;
    width: 90%;
    color: grey;
    line-height:36rpx;
    font-size: 16rpx;
    box-shadow: 1px 0 1px 1px lightgray;
    border-radius: 3rpx;
    padding:5%;
    margin-top:15rpx; 
    background: #fff;
}

